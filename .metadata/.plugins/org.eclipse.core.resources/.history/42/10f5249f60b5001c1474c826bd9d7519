package exception.handling;

import java.util.Scanner;

public class ExceptionHandlingExample1 {
	
	
	public static void main (String args[]) {
		
		Scanner sc = new Scanner(System.in);
		
		int no1 = sc.nextInt();
		int no2 =sc.nextInt();
		
		int no3 =no1/no2;
		
		System.out.println(no3);
		
		
	}

}
